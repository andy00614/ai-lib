{
  "name": "wd-ai-tools",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev --parallel",
    "build": "turbo build",
    "test": "turbo test --parallel",
    "lint": "turbo lint && manypkg check",
    "format": "biome format --write .",
    "typecheck": "turbo typecheck",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "dev:api": "turbo dev --filter=@wd-ai-tools/api",
    "dev:dashboard": "turbo dev --filter=@wd-ai-tools/dashboard"
  },
  "dependencies": {
    "turbo": "^2.5.0",
    "typescript": "^5.4.0",
    "@biomejs/biome": "^1.9.0",
    "@manypkg/cli": "^0.24.0"
  },
  "packageManager": "bun@1.2.21"
}